type User {
    id: ID!
}

type TravelPlan {
    id: ID!
    createdAt: DateTime!
    updatedAt: DateTime!
    title: String!
    owner: User!
    members: [User!]!
    days: [Day!]!

}

type Day {
    id: ID!
    date: Date
    dayOrder: NonNegativeInt!
    waypoints: [Waypoint!]!
    travelPlan: TravelPlan!
}

type Waypoint {
    id: ID!
    visitTime: LocalTime
    memo: String
    waypointOrder: NonNegativeInt!
    place: Place!
    day: Day!
}
