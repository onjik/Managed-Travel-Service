
type Country {
    id: ID!
    displayName: [LocalizedString!]!
    cities: [City!]!

}

type City {
    id: ID!
    displayName: [LocalizedString!]!
    country: Country!
    places: [Place!]!
}

type Place {
    id: ID!
    displayName: [LocalizedString!]!
    description: [LocalizedString!]!
    address: [LocalizedString!]!
    location: Point!
    city: City!
}